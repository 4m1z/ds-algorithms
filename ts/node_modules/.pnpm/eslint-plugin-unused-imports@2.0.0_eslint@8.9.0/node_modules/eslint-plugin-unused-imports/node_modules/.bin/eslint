#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint-plugin-unused-imports@2.0.0_eslint@8.9.0/node_modules/eslint-plugin-unused-imports/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint-plugin-unused-imports@2.0.0_eslint@8.9.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint@8.9.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint-plugin-unused-imports@2.0.0_eslint@8.9.0/node_modules/eslint-plugin-unused-imports/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/eslint-plugin-unused-imports@2.0.0_eslint@8.9.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.9.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.9.0/node_modules/eslint/bin/eslint.js" "$@"
fi
