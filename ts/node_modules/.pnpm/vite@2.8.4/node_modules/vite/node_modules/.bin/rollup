#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/dist/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/dist/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vite@2.8.4/node_modules/vite/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vite@2.8.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/dist/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/dist/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules/rollup/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/rollup@2.68.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vite@2.8.4/node_modules/vite/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vite@2.8.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.68.0/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.68.0/node_modules/rollup/dist/bin/rollup" "$@"
fi
