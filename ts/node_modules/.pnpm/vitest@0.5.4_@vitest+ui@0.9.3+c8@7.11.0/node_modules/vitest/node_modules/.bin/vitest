#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vitest@0.5.4_@vitest+ui@0.9.3+c8@7.11.0/node_modules/vitest/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vitest@0.5.4_@vitest+ui@0.9.3+c8@7.11.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vitest@0.5.4_@vitest+ui@0.9.3+c8@7.11.0/node_modules/vitest/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/vitest@0.5.4_@vitest+ui@0.9.3+c8@7.11.0/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
